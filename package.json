{
  "name": "RNBaseProject",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "newclear": "rm -rf $TMPDIR/react-* && watchman watch-del-all && rm -rf ios/build/ModuleCache/* && rm -rf node_modules/ && rm -rf ~/.rncache && yarn install && cd android && ./gradlew clean && yarn run fix-xcode",
    "createIconFont": "./node_modules/.bin/generate-icon ./src/assets/img/customFont/styles.css --componentName=icomoon --fontFamily=icomoon > ./node_modules/react-native-vector-icons/icomoon.js && cp ./src/assets/img/customFont/icomoon.ttf ./node_modules/react-native-vector-icons/Fonts/icomoon.ttf",
    "buildandroid": "cd android && ./gradlew clean && ./gradlew assembleRelease",
    "fix-xcode": "react-native-schemes-manager all",
    "postinstall": "react-native-schemes-manager all"
  },
  "dependencies": {
    "lodash": "^4.17.10",
    "moment": "^2.22.2",
    "prop-types": "^15.6.2",
    "react": "16.4.1",
    "react-native": "0.56.0",
    "react-native-animatable": "^1.3.0",
    "react-native-code-push": "^5.4.2",
    "react-native-config": "^0.11.5",
    "react-native-device-info": "^0.22.5",
    "react-native-i18n": "^2.0.15",
    "react-native-maps": "https://github.com/mikemorris/react-native-maps.git#rm-airgooglemaps",
    "react-native-navigation": "^2.0.2505",
    "react-native-vector-icons": "^5.0.0",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-persist": "^5.10.0",
    "redux-saga": "^0.16.0",
    "remotedev-serialize": "^0.1.1",
    "reselect": "^3.0.1",
    "seamless-immutable": "^7.1.3"
  },
  "xcodeSchemes": {
    "Debug": [
      "Debug"
    ],
    "Release": [
      "Staging",
      "Release"
    ],
    "projectDirectory": "iOS"
  },
  "devDependencies": {
    "ad-rn-generator": "^1.0.4",
    "babel-eslint": "^9.0.0",
    "babel-jest": "23.4.2",
    "babel-preset-react-native": "^5",
    "eslint": "^5.5.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-react-native": "^3.2.1",
    "jest": "23.5.0",
    "react-addons-test-utils": "^15.6.2",
    "react-native-schemes-manager": "^1.0.5",
    "react-test-renderer": "16.4.1",
    "redux-logger": "^3.0.6"
  },
  "jest": {
    "preset": "react-native"
  }
}
